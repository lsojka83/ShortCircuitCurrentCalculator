<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:layout_width="fill_parent">

    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_toEndOf="@+id/imageView"
        android:layout_alignBottom="@+id/imageView">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent" >

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/se_s"
                android:id="@+id/tileUtilityIV" />

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" >

                    <TextView

                        android:text="U. name"
                        android:id="@+id/tileUtilityNameTV"
                        android:layout_height="wrap_content"
                        android:textStyle="bold"
                        android:textColor="@color/colorPrimaryText"
                        android:textSize="20sp"
                        android:layout_marginTop="10dp"
                        android:layout_marginLeft="10dp"
                        android:layout_width="90dp">

                    </TextView>

                    <TableLayout
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent">

                        <TableRow
                            android:layout_width="match_parent"
                            android:layout_height="match_parent" >

                            <TextView
                                android:text="SkQ [MVA]"
                                android:id="@+id/textView"
                                android:layout_height="wrap_content"
                                android:textStyle="bold"
                                android:textColor="@color/colorPrimaryText"
                                android:textSize="10sp"
                                android:layout_width="110dp"
                                android:paddingLeft="5dp"
                                android:paddingTop="3dp">

                            </TextView>

                            <TextView
                                android:text="-"
                                android:id="@+id/utilityTileSCPowerTV"
                                android:layout_height="wrap_content"
                                android:textStyle="bold"
                                android:textColor="@color/colorPrimaryText"
                                android:textSize="10sp"
                                android:paddingLeft="3dp"
                                android:layout_width="wrap_content"
                                android:paddingTop="3dp">

                            </TextView>
                        </TableRow>

                        <TableRow
                            android:layout_width="match_parent"
                            android:layout_height="match_parent" >

                            <TextView
                                android:text="Un [kV]"
                                android:id="@+id/textView5"
                                android:layout_height="wrap_content"
                                android:textStyle="bold"
                                android:textColor="@color/colorPrimaryText"
                                android:textSize="10sp"
                                android:layout_width="match_parent"
                                android:paddingLeft="5dp"
                                android:paddingTop="3dp">

                            </TextView>

                            <TextView
                                android:text="-"
                                android:id="@+id/utilityTileNominalVoltageTV"
                                android:layout_height="wrap_content"
                                android:textStyle="bold"
                                android:textColor="@color/colorPrimaryText"
                                android:textSize="10sp"
                                android:paddingLeft="3dp"
                                android:layout_width="wrap_content"
                                android:paddingTop="3dp">

                            </TextView>
                        </TableRow>

                    </TableLayout>

                </TableRow>

                <TableLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <TableRow
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <TextView
                            android:text="ZkQ [\u03A9] "
                            android:id="@+id/textview10"
                            android:layout_height="wrap_content"
                            android:textStyle="bold"
                            android:textColor="@color/colorPrimaryText"
                            android:textSize="10sp"
                            android:layout_width="wrap_content"
                            android:layout_marginTop="3dp">

                        </TextView>

                        <TextView
                            android:text="-"
                            android:id="@+id/utilityTileImpedanceTV"
                            android:layout_height="wrap_content"
                            android:textStyle="bold"
                            android:textColor="@color/blue_dark"
                            android:textSize="10sp"
                            android:layout_width="wrap_content"
                            android:paddingLeft="3dp"
                            android:layout_marginTop="3dp">

                        </TextView>
                    </TableRow>

                    <TableRow
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <TextView
                            android:id="@+id/textview11"
                            android:layout_height="wrap_content"
                            android:textStyle="bold"
                            android:textColor="@color/colorPrimaryText"
                            android:textSize="10sp"
                            android:layout_width="wrap_content"
                            android:paddingLeft="10dp"
                            android:paddingTop="5dp"
                            android:layout_marginTop="3dp">

                        </TextView>

                        <TextView
                            android:id="@+id/utilityTileSCCurrentTV"
                            android:layout_height="wrap_content"
                            android:textStyle="bold"
                            android:textColor="@color/blue_dark"
                            android:textSize="12dp"
                            android:paddingLeft="10dp"
                            android:layout_width="wrap_content"
                            android:layout_marginTop="3dp">

                        </TextView>
                    </TableRow>

                </TableLayout>

            </LinearLayout>

        </TableRow>
    </TableLayout>


</LinearLayout>